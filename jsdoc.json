{
  "plugins": ["plugins/markdown", "jsdoc-plugin-typescript"],
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "\\.test\\."
  },
  "sourceType": "module",
  "opts": {
    "recurse": true,
    "destination": "./doc/"
  },
  "typescript": {
    "moduleRoot": "src"
  }
}
